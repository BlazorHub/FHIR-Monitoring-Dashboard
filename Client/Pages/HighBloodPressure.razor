@using FIT3077.Shared.Models

@if (Monitors != null)
{
    @foreach (var m in Monitors)
    {
        if (m.SystolicFlag == true && m.MeasurementList.BloodPressureRecords.IsMonitored == true)
        {
            <SystolicPressureChart Monitor="m"></SystolicPressureChart>
        }
    }
}
@code {
    [Parameter]
    public IReadOnlyList<Monitor> Monitors { get; set; }
}
